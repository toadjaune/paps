<table class="table table-responsive">
  <tr>
    <th>Nom</th>
    <th>Jour</th>
    <th>Bar</th>
    <th>Type</th>
    <th>Distance</th>
    <th>Adresse</th>
    <th>Commentaires</th>
  </tr>

  <% creneaus.each do |creneau| %>
    <tr>
      <td><%= link_to creneau, creneau %></td>
      <td><%= link_to creneau.jour %></td>
      <td><%= link_to creneau.bar if creneau.bar%></td>
      <td><%= creneau.genre %></td>
      <td><%= creneau.distance %></td>
      <td><%= creneau.adresse %></td>
      <td><%= creneau.commentaires %></td>
    </tr>
  <% end %>
</table>
